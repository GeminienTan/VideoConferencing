												<% if(conversation[i].receiver_id == u_id){ %>
												<a href="#"><img class="avatar-xl" src="<%= conversation[i].sender_profilepic %>" data-toggle="tooltip" data-placement="top" title="<%= conversation[i].sender_name %>" alt="avatar"></a>
												
													<% if(conversation[i].sender_status == 'A'){ %>
													<div class="status">
														<i class="material-icons online">fiber_manual_record</i>
													</div>
													<%}
													else{ %>
														<div class="status">
															<i class="material-icons offline">fiber_manual_record</i>
														</div>
